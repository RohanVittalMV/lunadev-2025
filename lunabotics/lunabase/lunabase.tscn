[sub_resource type="GDScript" id="GDScript_byehu"]
script/source = "extends GridContainer


var _first_selected := false
var _first_index := 0
var _first_button: Button

var _swaps: Array[Vector2i] = []


func _ready():
	if FileAccess.file_exists(\"swaps.dat\"):
		var file := FileAccess.open(\"swaps.dat\", FileAccess.READ)
		while file.get_position() < file.get_length():
			var value = file.get_var()
			if value is Vector2i:
				_swaps.push_back(value as Vector2i)
			else:
				push_error(\"swaps.dat is invalid!\")
				DirAccess.open(\".\").remove(\"swaps.dat\")
				break
	
	Lunabot.connected.connect(
		func():
			for swap in _swaps:
				Lunabot.swap_camera(swap.x, swap.y)
	)
	
	for i in range(6):
		var button := Button.new()
		button.text = str(i + 1)
		button.toggle_mode = true
		button.size_flags_horizontal = Control.SIZE_EXPAND_FILL
		
		button.toggled.connect(
			func(pressed: bool):
				if _first_selected:
					_first_selected = false
					_first_button.button_pressed = false
					button.button_pressed = false
					if pressed:
						Lunabot.swap_camera(_first_index, i)
						_swaps.append(Vector2i(_first_index, i))
					else:
						_first_selected = false
				elif pressed:
					_first_selected = true
					_first_index = i
					_first_button = button
		)
		add_child(button)


func _exit_tree() -> void:
	var file := FileAccess.open(\"swaps.dat\", FileAccess.WRITE)
	for swap in _swaps:
		file.store_var(swap)
	file.close()
"

