[sub_resource type="GDScript" id="GDScript_yu1su"]
script/source = "extends Label


var timer := 0.0


func _ready() -> void:
	Lunabot.something_received.connect(func():
		timer = 0
	)


func _process(delta: float) -> void:
	timer += delta
	text = \"Last Received: %s ms\" % [roundi(timer * 1000)]
